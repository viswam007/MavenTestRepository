pipeline{
  agent any
    stages {
      stage ("Checkout") {
         steps {
                 echo 'Checkout the code............'
             }
      }
      stage ("Build") {
         steps {
            echo 'Building the Application.........'
             }
      }
      
       stage ("Test") {
         steps {
            echo 'Testing the Application.........'
             }
      }
      
      stage ("Deploy") {
         steps {
            echo 'Deploy Application'
             }
    }
  }
}
